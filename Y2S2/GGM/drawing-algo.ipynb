   "source": [
    "def BresenhamLine(xi, yi, xf, yf):\n",
    "    dx = xf - xi\n",
    "    dy = yf - yi\n",
    "    points = []\n",
    "    d_values = []\n",
    "\n",
    "    d = 2 * dy - dx\n",
    "\n",
    "    y = yi\n",
    "    for x in range (xi, xf + 1):\n",
    "        points.append((x, y))\n",
    "        d_values.append(d)\n",
    "        \n",
    "        if d < 0:\n",
    "            d += 2 * dy\n",
    "        else:\n",
    "            d += 2 * (dy - dx)\n",
    "            y += 1\n",
    "    \n",
    "    print(points, \"\\n\", d_values)\n",
    "    return points\n",
    "        "
   ]
